	<servlet>
		<servlet-name>cadastrarViagem</servlet-name>
		<servlet-class>viagem.controller.viagemController</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>cadastrarViagem</servlet-name>
		<url-pattern>/cadastrarViagem</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>enviarContato</servlet-name>
