	</servlet>
	<servlet-mapping>
		<servlet-name>enviarContato</servlet-name>
		<url-pattern>/enviarContato</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>cadastrarCliente</servlet-name>
		<servlet-class>cliente.controller.clienteController</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>cadastrarCliente</servlet-name>
		<url-pattern>/cadastrarCliente</url-pattern>
	</servlet-mapping>
	
	
	
</web-app>